{
  "name": "weekday-keystone",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow bun",
    "dev": "bun run docker:start && bun run keystone:build && bun run db:prepare && next dev --hostname 0.0.0.0 --turbopack",
    "keystone:build": "keystone build --no-ui",
    "docker:start": "docker compose up -d && bun run docker:wait",
    "docker:wait": "until docker compose exec -T db pg_isready -U postgres > /dev/null 2>&1; do echo 'Waiting for database...'; sleep 1; done",
    "docker:stop": "docker compose down",
    "docker:clean": "docker compose down -v",
    "db:prepare": "bun scripts/db-prepare.ts",
    "build": "bun run keystone:build && bun run db:prepare && next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:gen": "bun run keystone:build && bun tsx features/keystone/view-order/generate-view-order.ts && keystone prisma migrate dev",
    "migrate": "dotenv -e .env.local -- prisma migrate deploy"
  },
  "packageManager": "bun@1.2.23",
  "trustedDependencies": [
    "@prisma/client",
    "prisma"
  ],
